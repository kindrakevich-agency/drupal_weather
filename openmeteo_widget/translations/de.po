msgid "Clear sky"
msgstr "Klarer Himmel"

msgid "Mainly clear"
msgstr "Überwiegend klar"

msgid "Partly cloudy"
msgstr "Teilweise bewölkt"

msgid "Overcast"
msgstr "Bedeckt"

msgid "Foggy"
msgstr "Neblig"

msgid "Drizzle"
msgstr "Nieselregen"

msgid "Freezing drizzle"
msgstr "Gefrierender Nieselregen"

msgid "Rain"
msgstr "Regen"

msgid "Freezing rain"
msgstr "Gefrierender Regen"

msgid "Snow"
msgstr "Schnee"

msgid "Snow grains"
msgstr "Schneegriesel"

msgid "Rain showers"
msgstr "Regenschauer"

msgid "Snow showers"
msgstr "Schneeschauer"

msgid "Thunderstorm"
msgstr "Gewitter"

msgid "Thunderstorm with hail"
msgstr "Gewitter mit Hagel"

msgid "Unknown"
msgstr "Unbekannt"
