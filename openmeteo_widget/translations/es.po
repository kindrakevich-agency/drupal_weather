msgid "Clear sky"
msgstr "Cielo despejado"

msgid "Mainly clear"
msgstr "Principalmente despejado"

msgid "Partly cloudy"
msgstr "Parcialmente nublado"

msgid "Overcast"
msgstr "Nublado"

msgid "Foggy"
msgstr "Neblinoso"

msgid "Drizzle"
msgstr "Llovizna"

msgid "Freezing drizzle"
msgstr "Llovizna helada"

msgid "Rain"
msgstr "Lluvia"

msgid "Freezing rain"
msgstr "Lluvia helada"

msgid "Snow"
msgstr "Nieve"

msgid "Snow grains"
msgstr "Granizo"

msgid "Rain showers"
msgstr "Chubascos"

msgid "Snow showers"
msgstr "Nevadas"

msgid "Thunderstorm"
msgstr "Tormenta"

msgid "Thunderstorm with hail"
msgstr "Tormenta con granizo"

msgid "Unknown"
msgstr "Desconocido"
